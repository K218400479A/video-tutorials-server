<div class="container">

    <main>
        <div class="jumbotron jumbotron-fluid">
            <div class="container">
                <h1 class="display-4">Video Tutorials</h1>
                <p class="lead">Start Self-Study Today: It'll Change Your Life</p>
            </div>
        </div>

        <div>
            <h1 class="text-center">Top Courses</h1>
            <div class="container mt-3">
                <div class="row d-flex justify-content-center">

                    <div class="card-deck">
                        {{#each videos}}
                        <div class="card mb-4">
                            {{!-- <img class="card-img-top" src="{{imgURL}}" alt="Card image cap" width="400"> --}}
                            <iframe class="card-iframe-top" height="56%" src="{{imgURL}}"
                                title="YouTube video player" frameborder="0"
                                allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture"
                                allowfullscreen></iframe>
                            <div class="card-body">
                                <h4 class="card-title">{{title}}</h4>
                            </div>
                        </div>
                        {{else}}
                        <h4 class="text-center">No courses...</h4>
                        {{/each}}
                    </div>

                </div>

            </div>
        </div>
    </main>

</div>



